{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"paths": {
			"@/*": ["./*"]
		},
		"types": ["@types/react", "@types/react-native"]
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"expo-env.ts"
	]
}
